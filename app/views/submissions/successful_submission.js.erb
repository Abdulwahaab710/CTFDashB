<% if @message&.empty? %>
  $('.alert').removeClass('alert-danger')
  $('.alert').addClass('alert-success')
  $('.alert').text('<%= flash[:success] %>')
  $('.alert').fadeIn()
<% else %>
  Swal.fire({
    title: '<%= flash[:success] %>',
    type: 'success',
    html: '<%= @message %>'
  })
<% end %>
